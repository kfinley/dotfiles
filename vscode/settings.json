{
  "testExplorer.addToEditorContextMenu": true,
  "dotnet-test-explorer.testProjectPath": "src/Tests/*/",
  "editor.formatOnSave": true,
  "emmet.includeLanguages": {
    "razor": "html"
  },
  "files.associations": {
    "*.cshtml": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.format.enable": false,
  "vetur.useWorkspaceDependencies": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "files.autoSave": "afterDelay",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.onTaskErrors": "showErrors",
  "workbench.startupEditor": "none",
  "react-native-storybooks.storyRegex": "packages/*/src/stories/*.stories.ts",
  "react-native-storybooks.port": 6007,
  "remote.SSH.configFile": "~/.ssh/config",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "files.insertFinalNewline": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "mssql.enableQueryHistoryCapture": false,
  "editor.formatOnPaste": true,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "cSpell.userWords": [
    "apigatewaymanagementapi",
    "backoff",
    "bootstrapper",
    "certificatemanager",
    "codespaces",
    "combinators",
    "dotfiles",
    "Groundboard",
    "Groundboarding",
    "holeshot",
    "interactjs",
    "inversify",
    "itemprop",
    "kfinley",
    "kiteboard",
    "Kiteboarding",
    "Kitesurfing",
    "kylefinley",
    "linkify",
    "Newtonsoft",
    "RACELOG",
    "registrer",
    "richkites",
    "Riverwalk",
    "sockette",
    "stepfunctions",
    "stickey",
    "Supercross",
    "Swipable",
    "Swipeable",
    "typeahead",
    "typemerger",
    "unmarshall",
    "vuex",
    "websockets"
  ],
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.splitInGroupLayout": "vertical",
  "aws.telemetry": false,
  "dotfiles.repository": "https://github.com/kfinley/dotfiles.git",
  "extensions.autoUpdate": false,
  "npm.keybindingsChangedWarningShown": true,
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode",
    "editor.wordWrap": "off"
  },
  "redhat.telemetry.enabled": false,
  "yaml.customTags": [
    "!And",
    "!And sequence",
    "!If",
    "!If sequence",
    "!Not",
    "!Not sequence",
    "!Equals",
    "!Equals sequence",
    "!Or",
    "!Or sequence",
    "!FindInMap",
    "!FindInMap sequence",
    "!Base64",
    "!Join",
    "!Join sequence",
    "!Cidr",
    "!Ref",
    "!Sub",
    "!Sub sequence",
    "!GetAtt",
    "!GetAZs",
    "!ImportValue",
    "!ImportValue sequence",
    "!Select",
    "!Select sequence",
    "!Split",
    "!Split sequence"
  ],
  "[python]": {
    "editor.formatOnType": true
  },
  "window.zoomLevel": -1
  "aws.profile": "profile:default",
  "workbench.colorTheme": "Default Dark+",
}
