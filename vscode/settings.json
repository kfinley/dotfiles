{
  "testExplorer.addToEditorContextMenu": true,
  "dotnet-test-explorer.testProjectPath": "src/Tests/*/",
  "editor.formatOnSave": true,
  "emmet.includeLanguages": {
    "razor": "html"
  },
  "files.associations": {
    "*.cshtml": "html"
  },
  "[javascript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[html]": {
    "editor.defaultFormatter": "vscode.html-language-features"
  },
  "diffEditor.ignoreTrimWhitespace": false,
  "[typescript]": {
    "editor.defaultFormatter": "vscode.typescript-language-features"
  },
  "[vue]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[json]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "javascript.updateImportsOnFileMove.enabled": "never",
  "typescript.updateImportsOnFileMove.enabled": "never",
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "json.format.enable": false,
  "vetur.useWorkspaceDependencies": true,
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editorAssociations": {
    "*.ipynb": "jupyter.notebook.ipynb"
  },
  "files.autoSave": "afterDelay",
  "[typescriptreact]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "debug.onTaskErrors": "showErrors",
  "workbench.startupEditor": "none",
  "react-native-storybooks.storyRegex": "packages/*/src/stories/*.stories.ts",
  "react-native-storybooks.port": 6007,
  "remote.SSH.configFile": "~/.ssh/config",
  "security.workspace.trust.untrustedFiles": "open",
  "terminal.integrated.defaultProfile.osx": "zsh",
  "terminal.integrated.defaultProfile.linux": "zsh",
  "files.insertFinalNewline": true,
  "explorer.confirmDelete": false,
  "explorer.confirmDragAndDrop": false,
  "editor.suggestSelection": "first",
  "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
  "mssql.enableQueryHistoryCapture": false,
  "editor.formatOnPaste": true,
  "[csharp]": {
    "editor.defaultFormatter": "ms-dotnettools.csharp"
  },
  "cSpell.userWords": [
    "apigatewaymanagementapi",
    "bootstrapper",
    "certificatemanager",
    "dotfiles",
    "Groundboard",
    "Groundboarding",
    "holeshot",
    "interactjs",
    "inversify",
    "itemprop",
    "kfinley",
    "kiteboard",
    "Kiteboarding",
    "Kitesurfing",
    "kylefinley",
    "Newtonsoft",
    "richkites",
    "sockette",
    "stepfunctions",
    "stickey",
    "Swipable",
    "Swipeable",
    "typeahead",
    "typemerger",
    "vuex",
    "websockets"
  ],
  "[scss]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "workbench.editor.splitInGroupLayout": "vertical",
  "aws.telemetry": false,
  "dotfiles.repository": "https://github.com/kfinley/dotfiles.git",
  "extensions.autoUpdate": false,
  "npm.keybindingsChangedWarningShown": true,
  "aws.profile": "profile:default",
  "[markdown]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  }
}
